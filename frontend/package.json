    "npm-check-updates": "^18.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.1.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vue-tsc": "^3.0.6",
    "webdriverio": "^9.19.2"
