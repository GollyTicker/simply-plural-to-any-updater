{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "export VITE_PLURALSYNC_BASE_URL=http://localhost:8080 && vite",
    "build": "run-p type-check \"build-only {@}\" --",
