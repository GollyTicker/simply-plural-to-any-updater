  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "export SP2ANY_BASE_URL=http://localhost:8080 && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
