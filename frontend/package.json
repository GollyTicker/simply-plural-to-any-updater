    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1",
    "webdriverio": "^9.20.0"
